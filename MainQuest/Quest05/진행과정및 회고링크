// ì±Œë¦°ì§€ ë°ì´í„° ëª¨ë¸
class Challenge {
  final String title;
  final int seconds;

  Challenge({required this.title, required this.seconds});
}

// ì±Œë¦°ì§€ ëª©ë¡ (ë‚˜ì¤‘ì— ì„œë²„ì—ì„œ ê°€ì ¸ì˜¬ ë°ì´í„°)
final List<Challenge> challenges = [Challenge(title: 'ë°œê°€ë½ìœ¼ë¡œ ì½”íŒŒê¸°', seconds: 30)];

class ChallengeService {
  late final GenerativeModel model;

  ChallengeService() {
    final apiKey = dotenv.env['GEMINI_API_KEY'] ?? '';
    // print(
    //   'ChallengeService - API í‚¤: ${apiKey.isEmpty ? "ë¹„ì–´ìˆìŒ" : "${apiKey.substring(0, 5)}..."}',
    // );

    model = GenerativeModel(model: 'gemini-pro', apiKey: apiKey);
  }

  Future<Challenge> getRandomChallenge() async {
    try {
      final prompt =
          'ì¬ë¯¸ìˆëŠ” 30ì´ˆì§œë¦¬ ì±Œë¦°ì§€ë‚˜ ë²Œì¹™ ê²Œì„ì„ í•˜ë‚˜ë§Œ ì œì•ˆí•´ì£¼ì„¸ìš”. "title"ê³¼ "seconds"ë¥¼ í¬í•¨í•˜ì—¬ JSON í˜•ì‹ìœ¼ë¡œ ì‘ë‹µí•´ì£¼ì„¸ìš”.';
      final content = [Content.text(prompt)];
      final response = await model.generateContent(content);

      // response.textê°€ nullì´ ì•„ë‹Œì§€ í™•ì¸
      final responseText = response.text;
      if (responseText == null) {
        throw Exception('API response is null');
      }

      // JSON íŒŒì‹± ë° Challenge ê°ì²´ ìƒì„±
      final jsonResponse = jsonDecode(responseText);
      return Challenge(
        title: jsonResponse['title'] as String,
        seconds: jsonResponse['seconds'] as int,
      );
    } catch (e) {
      // API í˜¸ì¶œ ì‹¤íŒ¨ì‹œ ê¸°ë³¸ ì±Œë¦°ì§€ ë°˜í™˜
      return challenges[Random().nextInt(challenges.length)];
    }
  }

  // ë²Œì¹™ ëª©ë¡
  final List<String> punishments = [
    'ì œìë¦¬ ì í”„ 20íšŒ í•˜ê¸°',
    '30ì´ˆ ë™ì•ˆ í•œ ë°œë¡œ ì„œìˆê¸°',
    '10ì´ˆ ë™ì•ˆ í˜€ ë‚´ë°€ê³  ìˆê¸°',
    'ë²½ì— ì†ìœ¼ë¡œ ë¬¼êµ¬ë‚˜ë¬´ì„œê¸°',
    'ê°œêµ¬ë¦¬ì²˜ëŸ¼ 5ë²ˆ ì í”„í•˜ê¸°',
    '15ì´ˆ ë™ì•ˆ ì¶¤ì¶”ê¸°',
    'ëˆˆ ê°ê³  ì½” í„°ì¹˜í•˜ê¸° 10íšŒ',
    '20ì´ˆ ë™ì•ˆ ì›ƒì§€ ì•Šê³  ìˆê¸°',
    'ì†ìœ¼ë¡œ ì•ŒíŒŒë²³ ëª¨ì–‘ ë§Œë“¤ê¸°',
    'ë™ë¬¼ ì†Œë¦¬ 3ê°€ì§€ ë‚´ê¸°',
    'ê±°ê¾¸ë¡œ ë§í•˜ê¸° ë„ì „',
    'ì†ê°€ë½ìœ¼ë¡œ ì½” ë§Œì§€ê³  ë±…ê¸€ë±…ê¸€ ëŒê¸°',
  ];

  Future<String> getRandomPunishment() async {
    final random = Random();
    return punishments[random.nextInt(punishments.length)];
  }
}

